<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>HarshP5</display-name>
	<servlet>
		<servlet-name>CookieServlet</servlet-name>
		<display-name>CookieServlet</display-name>
		<description></description>
		<servlet-class>cookies.CookieServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FinalOrder</servlet-name>
		<display-name>FinalOrder</display-name>
		<description></description>
		<servlet-class>cookies.FinalOrder</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FinalOrderInfo</servlet-name>
		<display-name>FinalOrderInfo</display-name>
		<description></description>
		<servlet-class>session.FinalOrderInfo</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SessionServlet</servlet-name>
		<display-name>SessionServlet</display-name>
		<description></description>
		<servlet-class>session.SessionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ResultSetTag</servlet-name>
		<display-name>ResultSetTag</display-name>
		<description></description>
		<servlet-class>customTags.ResultSetTag</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CookieServlet</servlet-name>
		<url-pattern>/CookieServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FinalOrder</servlet-name>
		<url-pattern>/FinalOrder</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FinalOrderInfo</servlet-name>
		<url-pattern>/FinalOrderInfo</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SessionServlet</servlet-name>
		<url-pattern>/SessionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResultSetTag</servlet-name>
		<url-pattern>/ResultSetTag</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
